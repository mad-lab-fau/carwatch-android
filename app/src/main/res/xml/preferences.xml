<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory
        android:key="cat_user_credentials"
        android:order="10"
        android:title="User Credentials">

        <!-- TODO REMOVE CODE BEFORE PUBLISHING!!!!!-->
        <EditTextPreference
            android:defaultValue="0406HR94"
            android:key="subject_id"
